{
  "name": "@ffx/cursor-sprint-templates",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index/index.es.js",
      "types": "./index.ts"
    },
    "./components": {
      "import": "./dist/components/index.es.js",
      "types": "./components/index.ts"
    },
    "./blueprints": {
      "import": "./dist/blueprints/index.es.js",
      "types": "./components/blueprints/index.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.es.js",
      "types": "./hooks/index.ts"
    },
    "./services": {
      "import": "./dist/services/index.es.js",
      "types": "./services/index.ts"
    },
    "./types": {
      "import": "./dist/types/index.es.js",
      "types": "./types/index.ts"
    },
    "./dist/cursor-sprint-templates.css": {
      "import": "./dist/cursor-sprint-templates.css"
    }
  },
  "files": [
    "dist",
    "index.ts",
    "components",
    "hooks",
    "services",
    "types",
    "integrations",
    "src"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build --config vite.config.ts",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "build:lib": "vite build",
    "lint": "eslint .",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
